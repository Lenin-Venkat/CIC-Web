                                                        });
                                                    });
                                                </script>
                                                <script>
                                                    var counterProject = document.getElementById("cWork").value;

                                                    $(function () {

                                                        $('#btnAddProjects').click(function () {
                                                            $(`<tr id="${counterProject}">
                                                                                                             <td class="row0">
                                                                                                             <input type="text" class="form-control" name="listOfPrevousClent[${counterProject}].NameofClient" value=""/>
                                                                                                             </td>
                                                                                                             <td class="row1">
                                                                                                             <input type="text" class="form-control" name="listOfPrevousClent[${counterProject}].ServiceProvided" value=" "  />
                                                                                                            </td>
                                                                                                            <td class="row2">
                                                                                                            <input type="text" class="form-control" name="listOfPrevousClent[${counterProject}].DateOrPeriod" id ="DateOrPeriod${counterProject}" value=" " />
                                                                                                            </td>
                                                                                                            <td class="row3">
                                                                                                            <input type="text" class="form-control" name="listOfPrevousClent[${counterProject}].ContractValue" value=" " />
                                                                                                            </td>
                                                                                                             <td>
                                                                                                            <button type="button" class="remove"><img src="/images/trush.svg" alt="remove row" /></button>
                                                                                                            </td>
                                                                                                            </tr><script>$(function () {
                                                        $('#DateOrPeriod${counterProject}').datepicker({



                                                            changeMonth: true,
                                                            changeYear: true,
                                                            format: "dd/mm/yyyy",
                                                            language: "tr"
                                                        });




                                                    });<\/script>`).appendTo('#listofPreviousproject');
                                                            counterProject++;
                                                            document.getElementById("cWork").value = counterProject;
                                                            return false;
                                                        });
                                                    });

                                                    $(document).ready(function () {

                                                        $('#tbodyList').on('click', '.remove', function () {

                                                            // containing the clicked button
                                                            var child = $(this).closest('tr').nextAll();

                                                            child.each(function () {

                                                                // Getting <tr> id.
                                                                var id = $(this).attr('id');

                                                                // Getting the <p> inside the .row-index class.
                                                                var idx = $(this).children('.row0').children('input');
                                                                var idx1 = $(this).children('.row1').children('input');
                                                                var idx2 = $(this).children('.row2').children('input');
                                                                var idx3 = $(this).children('.row3').children('input');
                                                                //var idx4 = $(this).children('.row5').children('input');
                                                                //var idx5 = $(this).children('.row6').children('input');
                                                                //var idx6 = $(this).children('.row7').children('input');

                                                                // Gets the row number from <tr> id.
                                                                var dig = parseInt(id);

                                                                var cnew = dig - 1;
                                                                // Modifying row index.
                                                                //idx.html(`Row ${dig - 1}`);
                                                                idx.attr('name', `listOfPrevousClent[${cnew}].NameofClient`)
                                                                idx1.attr('name', `listOfPrevousClent[${cnew}].ServiceProvided`)
                                                                idx2.attr('name', `listOfPrevousClent[${cnew}].DateOrPeriod`)
                                                                idx3.attr('name', `listOfPrevousClent[${cnew}].ContractValue`)
                                                                //idx4.attr('name', `worksCapability[${cnew}].ContractSum`)
                                                                //idx5.attr('name', `worksCapability[${cnew}].TelephoneNo`)
                                                                //idx6.attr('name', `worksCapability[${cnew}].TypeofInvolvement`)

                                                                // Modifying row id.
                                                                $(this).attr('id', `${dig - 1}`);
                                                            });

                                                            // Removing the current row.
                                                            $(this).closest('tr').remove();

                                                            // Decreasing total number of rows by 1.
                                                            counterProject--;
                                                            document.getElementById("cWork").value = counterProject;

                                                        });
                                                    });
                                                </script>

