<script type="text/javascript">
    $('#renewcheklist').hide();
    function handleClick(myRadio)
    {
        var value = myRadio.value;
        if (value == 'Renewal') {
            $('#newcheklist').hide();
            $('#renewcheklist').show();
            window.location.href = '/Cicform7/GetData?apptype=' + value;
        }
        else {
            $('#newcheklist').show();
            $('#renewcheklist').hide();
        }
    }
</script>










