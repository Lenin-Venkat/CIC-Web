                                <a href="#" class="btn btn-header-link" data-toggle="collapse" data-target="#prevtarget1"
                                   aria-expanded="true" aria-controls="prevtarget1">SECTION A - Details Of contractor liable</a>
                            </div>

                              <div id="prevtarget1" class="collapse show" aria-labelledby="prevhead1" data-parent="#Previewmaindetails">
                                <div class="card-body">
                                    <div class="fieldset">
                                        <div class="row">
                                            <div class="col-lg form-group">Contractor/Individual Artisan/Company/Organization</div>
                                            <div class="col-lg form-group">@Model.tabb1.Oraganization</div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg form-group">Category </div>
                                            <div class="col-lg form-group">@Model.SelectedCategory</div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg form-group">Grade </div>
                                            <div class="col-lg form-group">@Model.tabb1.Grade</div>
                                        </div>

                                        <div class="row">
                                            <div class="col-lg form-group">Certificate No</div>
                                            <div class="col-lg form-group">@Model.tabb1.CertificateNo</div>
